    "ts-jest": "^29.1.0",
    "typedoc": "^0.24.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "chalk": "^5.2.0",
    "commander": "^10.0.0",
    "enquirer": "^2.3.6"
  }
